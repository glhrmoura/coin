{
  "manifest_version": 3,
  "version": "0.1.11",
  "name": "Coin: Always be up to date",
  "description": "Monitoring the quotes of the main currencies in the world.",
  "author": "Guilherme Moura <mouraggui@gmail.com> (https://github.com/glhrmoura)",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "assets/icons/icon-16.png"
  },
  "icons": {
    "16": "assets/icons/icon-16.png",
    "32": "assets/icons/icon-32.png",
    "48": "assets/icons/icon-48.png",
    "128": "assets/icons/icon-128.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": ["storage"]
}